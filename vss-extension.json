{
  "manifestVersion": 1,
  "id": "Promote",
  "name": "Promote",
  "description": "Tool to allow the promotion of a package to a specified view.",
  "version": "1.6.0",
  "publisher": "Haplo",
  "Public": true,
  "repository": {
    "type": "git",
    "uri": "https://github.com/Haplo-tech/promote"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "tags": [
    "promote",
    "package",
    "nuget",
    "npm",
    "python",
    "pypi",
    "release"
  ],
  "links": {
    "home": {
      "uri": "https://haplo.com.au"
    },
    "repository": {
      "uri": "https://github.com/Haplo-tech/promote"
    },
    "issues": {
      "uri": "https://github.com/Haplo-tech/promote/issues"
    }
  },
  "categories": [
    "Azure Pipelines"
  ],
  "branding": {
    "color": "rgb(10, 219, 212)",
    "theme": "light"
  },
  "icons": {
    "default": "images/icon.png"
  },
  "screenshots": [
    {
      "path": "images/how-to.png"
    }
  ],
  "files": [
    {
      "path": "promote-package"
    }
  ],
  "contributions": [
    {
      "id": "promote-build-release-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "promote-package"
      }
    }
  ]
}